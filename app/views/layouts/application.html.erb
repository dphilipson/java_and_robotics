<!DOCTYPE html>
<html class="dark">
<head>
  <title><%= full_title(yield(:title)) %></title>
  <%= favicon_link_tag 'damping_matrix.jpg' %>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
</head>
<body>
  <%= render 'layouts/header' %>
  <div class="<%= (defined? @fluid) ? 'container-fluid' : 'containter' %>">
    <%= yield %>
    <%= render 'layouts/footer' %>
  </div>
</body>
</html>
